{
  "name": "FlyCheap Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": [
    "sidePanel"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.google.com/travel/flights/booking*"],
      "js": ["contentScript.js"]
    }
  ]
}
